{
  "name": "air-quality",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "compile": "shx rm -rf lib/ && tsc",
    "build": "tsc && rsync -a --exclude='*.ts' src/ dist/",
    "start": "npm run compile && npm run build && node dist/index.js",
    "test": "npx jest",
    "test:watch": "jest --watch",
    "db:create": "sequelize db:create",
    "db:migrate": "sequelize db:migrate",
    "db:migrate:undo": "sequelize db:migrate:undo:all",
    "db:new:migrattion": "npx sequelize migration:generate --name airQuality",
    "generate-migrations": "export $(grep -v '^#' .env | xargs) && npx sequelize-auto-migrations -d $DB_DATABASE -h $DB_HOST -u $DB_USERNAME -p $DB_PASSWORD --dialect $DB_DIALECT",
    "swagger-autogen": "node ./dist/api/controllers/swagger.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/node": "16.10.5",
    "@types/validator": "13.7.10",
    "@typescript-eslint/typescript-estree": "^5.44.0",
    "axios": "^1.6.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "mongoose": "^8.0.3",
    "node-cron": "^3.0.3",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.2.1",
    "sequelize": "^6.28.1",
    "sequelize-typescript": "^2.1.5",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "typescript-eslint": "0.0.1-alpha.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.37",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/debug": "~4.1.7",
    "@types/express": "~4.17.14",
    "@types/jest": "^29.4.0",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/morgan": "^1.9.3",
    "@types/node": "16.10.5",
    "@types/validator": "^13.7.6",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.28.0",
